my $s = prompt.words».Str;
my $a = $s.substr(*-1, 1).Int;
if $a ∈ [2, 4, 5, 7, 9] {
    say 'hon';
}
elsif $a ∈ [0, 1, 6, 8] {
    say 'pon';
}
else {
    say 'bon';
}
